local function SAEAMKY_fake_script() -- Binder.bindertrans 
	local script = Instance.new('LocalScript', Binder)

	local button = script.Parent -- Ð­Ñ‚Ð¾ Ð´Ð¾Ð»Ð¶ÐµÐ½ Ð±Ñ‹Ñ‚ÑŒ ÑÐ°Ð¼ TextButton "Binder"
	
	-- Ð£ÑÑ‚Ð°Ð½Ð¾Ð²ÐºÐ° Ð¿Ñ€Ð¾Ð·Ñ€Ð°Ñ‡Ð½Ð¾ÑÑ‚Ð¸ ÑÑ€Ð°Ð·Ñƒ Ð¿Ñ€Ð¸ Ð¸Ð½Ð¶ÐµÐºÑ‚Ðµ
	button.BackgroundTransparency = 0.7
	
